class Animal {
    public void sound() { 
        System.out.println("Animal sound"); 
    }
}

class Dog extends Animal {
    public void sound() { 
        System.out.println("Bark"); 
    }
}

public class Activity6 {
    public static void main(String[] args) {
        Animal animalObj = new Animal();   // changed from 'a' to 'animalObj'
        Animal dogObj = new Dog();         // changed from 'd' to 'dogObj'
        
        animalObj.sound();  // calls Animal's sound
        dogObj.sound();     // calls Dog's sound
    }
}
